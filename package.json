{
  "name": "px2agent",
  "version": "2.0.7",
  "description": "Pickles 2 と NodeJS スクリプトを仲介するAPIを提供します。",
  "private": false,
  "main": "dist/px2agent.js",
  "module": "dist/px2agent.mjs",
  "types": "dist/px2agent.d.ts",
  "type": "module",
  "exports": {
    "import": "./dist/px2agent.mjs",
    "require": "./dist/px2agent.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/pickles2/node-px2agent.git"
  },
  "scripts": {
    "build": "tsc && node build.js",
    "test": "npm run build && node --experimental-vm-modules node_modules/.bin/jest",
    "documentation": "typedoc --out docs src/",
    "prepublishOnly": "npm run build"
  },
  "author": "Tomoya Koyanagi",
  "license": "MIT",
  "dependencies": {
    "child_process": "^1.0.2",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "node-php-bin": "^0.2.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.11",
    "typescript": "^5.3.3"
  }
}
